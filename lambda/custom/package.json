{
  "name": "pain-diary",
  "version": "1.0.0",
  "private": true,
  "description": "Alexa skill to record pain scores in diary",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "prepush": "npm run lint && npm test"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "keywords": [
    "alexa",
    "skill",
    "trivia"
  ],
  "author": "Steven Beales",
  "license": "ISC",
  "dependencies": {
    "ask-sdk-core": "^2.0.0",
    "ask-sdk-model": "^1.0.0",
    "ask-sdk-dynamodb-persistence-adapter": "^2.0.7",
    "i18next": "^10.6.0",
    "i18next-sprintf-postprocessor": "^0.2.2"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "husky": "^0.14.3",
    "jest": "^23.3.0"
  }
}
